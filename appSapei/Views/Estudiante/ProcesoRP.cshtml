<div class="modal fade bd-example-modal-lg" id="AltaPrograma" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h3 class="modal-title" id="exampleModalLongTitle">Alta de proyecto</h3>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form id="frmAltaProyecto" data-parsley-validate class="form-horizontal form-label-left">
                <div class="modal-body">
                    <div class="form-group">
                        <div class="col-md-4 col-lg-4 col-xs-12">
                            <label>RFC de la Dependencia</label>
                            <input type="text" class="form-control" id="RFCtxt" disabled />
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <label>Nombre del proyecto</label>
                            <input type="text" class="form-control" maxlength="200" id="NombreProyectotxt" placeholder="Nombre del Proyecto" style="text-transform:uppercase;" onkeyup="javascript:this.value=this.value.toUpperCase();" />
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <label>Correo Electrónico</label>
                            <input type="email" class="form-control" id="Correotxt" maxlength="60" placeholder="correo@dominio.com" oninvalid=" ValidaCampo(this, 'CORREO');" onblur="     ValidaCampo(this, 'CORREO');" required />
                        </div>

                    </div>
                    <div class="form-group">
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <label>Departamento Solicitado</label>
                            <input type="text" class="form-control" id="Departamentotxt" maxlength="100" placeholder="Departamento Solicitado" required />
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <label>Nombre del responsable del programa</label>
                            <input type="text" class="form-control" id="Responsabletxt" maxlength="60" placeholder="Nombre del responsable del programa" required />
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <label>Cargo del responsable del programa</label>
                            <input type="text" class="form-control" id="CargoResponsabletxt" maxlength="60" placeholder="Cargo del responsable del programa" required />
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <label>Opción de proyecto</label>
                            <select class="form-control" id="cboopcion">
                                <option selected></option>
                                <option value="banco">Banco de proyectos</option>
                                <option value="propi">Propuesta propia</option>
                                <option value="traba">Trabajador</option>
                            </select>
                        </div>
                        <div class="col-md-4 col-sm-4 col-xs-12">
                            <input type="hidden" id="txtId" />
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <button type="submit" id="btnGuardar" class="btn btn-primary">Guardar</button>
                </div>
            </form>
        </div>
    </div>
</div>

