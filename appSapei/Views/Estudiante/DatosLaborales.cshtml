<div class="row">
     <div class="col-md-4 col-sm-4 col-xs-12 form-group">
          <label for="txtNombreEmpresa">
               Nombre de la Empresa
          </label>
          <input type="text" class="form-control" id="txtNombreEmpresa" placeholder="Nombre de la empresa" />
          <div class="help-block with-errors"></div>
     </div>
     <div class="col-md-4 col-sm-4 col-xs-12 form-group">
          <label for="txtDomicilioEmpresa">
               Domicilio (Calle y Número)
          </label>
          <input type="text" class="form-control" id="txtDomicilioEmpresa" placeholder="Calle y número" />
          <div class="help-block with-errors"></div>
     </div>
     <div class="col-md-4 col-sm-4 col-xs-12 form-group">
          <label for="txtCodPostalEmpresa">
               Código Postal
          </label>
          <input type="number" class="form-control" id="txtCodPostalEmpresa" maxlength="5" placeholder="Código Postal" onblur="autocompleta(this, 'Empresa');" />
          <div class="help-block with-errors"></div>
     </div>
</div>
<div class="row">
     <div class="col-md-4 col-sm-4 col-xs-12 form-group">
          <label for="cboColoniaEmpresa">
               Colonia
          </label>
          <select class="form-control" id="cboColoniaEmpresa" placeholder="Ingrese primero su código postal"></select>
     </div>
     <div class="col-md-4 col-sm-4 col-xs-12 form-group">
          <label for="txtCiudadEmpresa">
               Ciudad o Localidad
          </label>
          <input type="text" class="form-control" id="txtCiudadEmpresa" disabled placeholder="Ciudad o Localidad" />
          <div class="help-block with-errors"></div>
     </div>
     <div class="col-md-4 col-sm-4 col-xs-12 form-group">
          <label for="txtEstadoEmpresa">
               Entidad Federativa
          </label>
          <input type="text" class="form-control" id="txtEstadoEmpresa" disabled placeholder="Entidad Federativa" />
          <div class="help-block with-errors"></div>
     </div>
</div>
