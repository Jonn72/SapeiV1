<div class="">
     <div class="page-title">
          <div class="title_left">
               <h3>Níveles de Inglés</h3>
          </div>
     </div>

     <div class="clearfix"></div>
     <form class="form-horizontal form-label-left" action="#" id="frmCLENiveles" role="form" data-toggle="validator" method="post" accept-charset="utf-8">
          <div class="row">
               <div class="col-md-4 col-sm-12 col-xs-12">
                    <div class="x_panel">
                         <div class="x_title">
                              <h2>Registro</h2>
                              <div class="clearfix"></div>
                         </div>
                         <div class="x_content">
                              <div class="form-group ">
                                   <label>
                                        Nivel
                                   </label>
                                   <input type="text" class="form-control" id="txtNivel" placeholder="Nivel" required />
                                   <label>
                                        Seriación (No debe estar duplicada) 
                                   </label>
                                   <input type="number" class="form-control" id="txtDescripcion" placeholder="Descripción" required />
                              </div>
                         </div>
                         <div class="row">
                              <div class="col-md-6">
                                   <button id="btnNuevo" type="submit" class="btn btn-block btn-info">Nuevo</button>
                              </div>
                              <div class="col-md-6">
                                   <button id="btnGuardar" type="submit" class="btn btn-block btn-success">Guardar</button>
                              </div>
                         </div>
                    </div>                    
               </div>
               <div class="col-md-8 col-sm-12 col-xs-12">
                    @Html.Partial("../Generales/TablaGeneral")
               </div>
          </div>
     </form>
</div>
<script src="~/Scripts/ManejoDatos/CentroLenguasExt/Niveles.js"></script>