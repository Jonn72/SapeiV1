

<input type="hidden" id="hidPeriodo" value="@ViewData["txtPeriodo"]" />
<div class="page-title">
    <div class="title_left">
        <h3>Fechas de Reinscripción <small>@ViewData["txtDescPeriodo"]</small></h3>
    </div>

</div>
<div class="clearfix"></div>
<form class="form-horizontal form-label-left" action="#" id="frmPeriodos" method="post" accept-charset="utf-8">
        <div class="x_panel">
                    <div class="x_title">
                        <h4>Registro</h4>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <div class="col-md-4 col-sm-6 col-xs-12">
                            @Html.Partial("../Combos/Carreras")
                        </div>

                        <div class="col-md-4 col-sm-6 col-xs-12">
                            <label>
                                Inicio
                            </label>
                            <div class='input-group date' id='dtpFechaInicio'>
                                <input type='text' id="txtFechaInicio" class="form-control" required />
                                <span class="input-group-addon"> <span class="glyphicon glyphicon-calendar"></span> </span>
                            </div>
                        </div>
                        <div class="col-md-2 col-sm-3 col-xs-12">
                            <label>
                                Intervalo 
                            </label>
                            <input type="number" class="form-control" id="txtIntervalo" min="1" value="0" required />
                        </div>
                        <div class="col-md-2 col-sm-3 col-xs-12">
                            <label>
                                Estudiantes
                            </label>
                            <input type="number" class="form-control" min="1" id="txtPersonas" value="0" required />
                        </div>
                    </div>
                </div>
            

        
        <div class="row">
            <div class="col-md-offset-8 col-md-4">
                <button id="btnGuardar" type="submit" class="btn btn-block btn-success">Guardar Fechas</button>
            </div>
        </div>
    
    @Html.Partial("../Generales/TablaGeneral")
</form>

<script src="~/Scripts/ManejoDatos/DEP/ReinscripcionFechasCarreras.js"></script>


