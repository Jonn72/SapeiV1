<div class="page-title">
     <div class="title_left">
          <h3>Datos <small>de escuela de procedencia</small></h3>
     </div>
</div>
<div class="clearfix"></div>
<div class="x_panel">
     <form class="form-horizontal form-label-left" action="#" id="frmDatosEscuela" role="form" data-toggle="validator" method="post" accept-charset="utf-8">
          <input type="hidden" id="idEscuela" value="@ViewData["idEscuela"]" />
          <div class="x_title">
               <div class="row">
                    <div class="col-md-6 col-sm-6 col-xs-12 form-group">
                         <label for="cboTipoEscuelaProcedencia">
                              Escuela de Procedencia
                         </label>
                         <select class="form-control" id="cboTipoEscuelaProcedencia"></select>
                    </div>
               </div>
               <div class="row">
                    <div class="col-md-4 col-sm-4 col-xs-12 form-group">
                         <label for="txtAnioEgreso">
                              Año de Egreso
                         </label>
                         <input type="text" class="form-control" id="txtAnioEgreso" value="@ViewData["egreso"]" placeholder="Año de Egreso" oninvalid="ValidaCampo(this, 'AÑO');" onblur="     ValidaCampo(this, 'AÑO');" required />
                         <div class="help-block with-errors"></div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12 form-group">
                         <label for="txtPromedio">
                              Promedio
                         </label>
                         <input type="text" class="form-control" id="txtPromedio" value="@ViewData["promedio"]" placeholder="Promedio" oninvalid="ValidaCampo(this, 'AVG');" onblur="     ValidaCampo(this, 'AVG');" required />
                         <div class="help-block with-errors"></div>
                    </div>
               </div>
          </div>
          <div class="row">
               <div class="col-md-4 col-md-offset-4">
                    <button id="btnAnterior" type="submit" class="btn btn-block btn-info">Atras</button>
               </div>
               <div class="col-md-4">
                    <button id="btnGuardar" type="submit" class="btn btn-block btn-success">Finalizar</button>
               </div>
          </div>
     </form>
</div>
<script src="~/Scripts/ManejoDatos/Aspirantes/DatosEscuela.js"></script>


