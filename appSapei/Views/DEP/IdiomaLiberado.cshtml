<div class="page-title">
    <div class="title_left">
        <h3>Avance  <small>de inglés</small></h3>
    </div>
</div>
<div class="clearfix"></div>

@{
    System.Data.DataRow loFila = null;
    string lsLiberacion = "";
    string lsFecha = "";
    string lsCalificacion = "";
    string lsMensaje = "";

    if (ViewData["DatosIngles"] != null)
    {
        loFila = (ViewData["DatosIngles"] as System.Data.DataTable).Rows[0];
        lsLiberacion = loFila.ItemArray[0].ToString().Trim();
        lsFecha = loFila.ItemArray[1].ToString().Trim();
        lsCalificacion = loFila.ItemArray[2].ToString().Trim();
        lsMensaje = loFila.ItemArray[3].ToString().Trim();

    }
}

<div class="x_panel">
    <div class="x_content">
        <div class="col-lg-6 col-md-6 center-margin">
            <div class="row">
                <ul class="stats-overview">

                    <li style="width:30%">
                        <span class="name">Liberación</span>
                        <span class="value text-success"> @lsLiberacion </span>
                    </li>
                    <li style="width:30%">
                        <span class="name"> Fecha </span>
                        <span class="value text-success"> @lsFecha </span>
                    </li>
                    <li style="width:30%">
                        <span class="name"> Calificación</span>
                        <span class="value text-success"> @lsCalificacion </span>
                    </li>
                </ul>
                <br />
            </div>


                <div class="col-md-8 center-margin center-block">
                    <div class="alert alert-info alert-dismissible fade in" role="alert">
                        <h4 class="title text-center">
                            @lsMensaje.
                        </h4>
                    </div>
                </div>

        </div>

    </div>

</div>
