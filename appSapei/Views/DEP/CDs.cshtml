
<div class="">
    <div class="page-title">
        <div class="title_left">
            <h3>CD's</h3>
        </div>
    </div>
    <div class="clearfix"></div>
    <form class="form-horizontal form-label-left" action="#" id="frmCds" role="form" data-toggle="validator" method="post" accept-charset="utf-8">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Editar</h2>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">

                        <input type="hidden" id="hidId" value="0" />
                        <input type="hidden" id="hidIdMaterial" value="0" />
                     

                        <div class="form-group col-md-6 ">
                            <label for="txtDescripcion">
                                Descripción
                            </label>
                            <input type="text" class="form-control" id="txtDescripcion" required />
                        </div>
                        <div class="form-group col-md-6 ">
                            <label for="txtDuracion">
                                Duración
                            </label>
                            <input type="number" step="0.01" class="form-control" id="txtDuracion" placeholder="Ingrese Lugar" required />
                        </div>

                        <div class="form-group mb-2">
                            <div class="col-md-4 col-sm-4 col-xs-12 ">
                                <br />
                                <button id="btnGuardar" type="submit" class="btn btn-info mb-2">Guardar</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>

    @Html.Partial("ModalElimina")



</div>
@Html.Partial("../Generales/TablaGeneral")
<script src="~/Scripts/ManejoDatos/CentroInformacion/CDs.js"></script>
