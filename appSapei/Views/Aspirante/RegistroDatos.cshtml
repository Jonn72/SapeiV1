                                            <div class="row">
                                                <div class="col-lg-4 col-md-3 col-sm-12 col-xs-12 text-align-left">
                                                    <label>Escuela de Egreso</label>
                                                </div>
                                                <div class="col-lg-8 col-md-9 col-sm-12 col-xs-12 text-align-left">
                                                    @if (string.IsNullOrEmpty(lsEsculaProcedencia))
                                                    {
                                                        <div class="input-icon">
                                                            <select class="list-dt" id="cboEscuelaProcedencia">
                                                                @{
                                                                    DataTable loEscuelas;
                                                                    Int16 lsValor = 0;
                                                                    string lsDescripcion = "";
                                                                    if (ViewData["EscuelasProcedencia"] != null)
                                                                    {
                                                                        loEscuelas = ViewData["EscuelasProcedencia"] as DataTable;
                                                                        foreach (DataRow loRow in loEscuelas.Rows)
                                                                        {
                                                                            lsValor = loRow.Field<Int16>("valor");
                                                                            lsDescripcion = loRow.Field<string>("descripcion");
                                                                            <option value="@lsValor">@lsDescripcion</option>
                                                                        }
                                                                    }
                                                                }
                                                            </select>
                                                            <i class="fa fa-exclamation-circle icon orange"> </i>
