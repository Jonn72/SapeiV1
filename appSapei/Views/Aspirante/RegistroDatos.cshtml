@using Sapei.Framework.Utilerias;
@using System.Data;


@{ int liEstado = 0;
    string lsNombre = "";
    string lsPaterno = "";
    string lsMaterno = "";
    string lsCurp = "";
    string lsFechaNacimiento = "";
    //Personales
    string lsEstadoCivil = "";
    string lsCorreo = "";
    string lsTelefono = "";
    string lsTelefonoEmergencia =  "";
    string lsCelular = "";
    string lsNss = "";
    //Domicilio
    string lsCalle = "";
    string lsNumero = "";
    string lsCp = "";
    string lsEntidad = "";
    string lsColonia = "";
    string lsLocalidad = "";

    //Seleccion de carrera
    string lsComoEnteraste = "";
    string lsCarrera1 = "";
    string lsCarrera2 = "";
    //Escuela procedencia
    string lsEsculaProcedencia = "";
    string lsPromedio = "";
    string lsAñoEgreso = "";


    DataRow loFila;
    if (ViewData["Datos"] != null)
    {
        loFila = (ViewData["Datos"] as DataTable).Rows[0];
        liEstado = Convert.ToInt32(loFila.Field<Int16>("estado"));
        lsNombre = loFila.Field<string>("nombre");
        lsPaterno = loFila.Field<string>("paterno");
        lsMaterno = loFila.Field<string>("materno");
        lsCurp = loFila.Field<string>("curp");
        lsFechaNacimiento = loFila.Field<string>("fecha_nacimiento");
        if (liEstado > 0)
        {
            lsEstadoCivil = loFila.Field<string>("estado_civil");
            lsCorreo = loFila.Field<string>("correo");
            lsTelefono = loFila.Field<string>("telefono");
            lsTelefonoEmergencia = loFila.Field<string>("telefono_emergencia");
            lsCelular = loFila.Field<string>("celular");
            lsNss = loFila.Field<string>("nss");

            if (liEstado > 1)
            {
                lsCalle = loFila.Field<string>("calle");
                lsNumero = loFila.Field<string>("numero");
                lsCp = Convert.ToString(loFila.Field<int>("cp"));
                lsColonia = loFila.Field<string>("colonia");
                lsLocalidad = loFila.Field<string>("ciudad");
                lsEntidad = loFila.Field<string>("entidad");
                if (liEstado > 2)
                {
                    lsComoEnteraste = loFila.Field<string>("enteraste");
                    lsCarrera1 = loFila.Field<string>("carrera1");
                    lsCarrera2 = loFila.Field<string>("carrera2");

                    if (liEstado > 3)
                    {
                        lsEsculaProcedencia = loFila.Field<string>("escuela_procedencia");
                        lsPromedio = loFila.Field<string>("promedio");
                        lsAñoEgreso = loFila.Field<string>("anio_egreso");
                    }
                }

            }
        }

    }
}




