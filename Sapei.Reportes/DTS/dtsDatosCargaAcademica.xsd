          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="DataTable1TableAdapter" GeneratorDataComponentClassName="DataTable1TableAdapter" Name="DataTable1" UserDataComponentName="DataTable1TableAdapter">
            <MainSource>
              <DbSource ConnectionRef="bdprueba (Settings)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select A.no_de_control,(nombre_alumno+' '+apellido_paterno+' ' +apellido_materno) AS nombre,A.semestre, A.reticula,A.especialidad, 
(SELECT nombre_carrera FROM carreras WHERE (carrera = A.carrera) AND (reticula = A.reticula)) AS nombre_carrera, 
(select clave_oficial from carreras where carrera = A.carrera and reticula = A.reticula) plan_estudios,materia, 
(select nombre_abreviado_materia from materias where materia = S.materia) nom_materia,grupo, repeticion, 
(select creditos_materia from materias_carreras where carrera = A.carrera and reticula = A.reticula and materia = S.materia) cr , 
isnull((select substring(convert(varchar(17),hora_inicial),12,5)+' - '+ substring(convert(varchar(17),hora_final),12,5)+' / '+aula horario from horarios where periodo = S.periodo AND materia = S.materia and grupo = S.grupo and dia_semana = 2),'') lunes , 
isnull((select substring(convert(varchar(17),hora_inicial),12,5)+' - '+ substring(convert(varchar(17),hora_final),12,5)+' / '+aula horario from horarios where periodo = S.periodo AND materia = S.materia and grupo = S.grupo and dia_semana = 3),'') martes, 
isnull((select substring(convert(varchar(17),hora_inicial),12,5)+' - '+ substring(convert(varchar(17),hora_final),12,5)+' / '+aula horario from horarios where periodo = S.periodo AND materia = S.materia and grupo = S.grupo and dia_semana = 4),'') miercoles, 
isnull((select substring(convert(varchar(17),hora_inicial),12,5)+' - '+ substring(convert(varchar(17),hora_final),12,5)+' / '+aula horario from horarios where periodo = S.periodo AND materia = S.materia and grupo = S.grupo and dia_semana = 5),'') jueves  , 
isnull((select substring(convert(varchar(17),hora_inicial),12,5)+' - '+ substring(convert(varchar(17),hora_final),12,5)+' / '+aula horario from horarios where periodo = S.periodo AND materia = S.materia and grupo = S.grupo and dia_semana = 6),'') viernes  , 
isnull((select substring(convert(varchar(17),hora_inicial),12,5)+' - '+ substring(convert(varchar(17),hora_final),12,5)+' / '+aula horario from horarios where periodo = S.periodo AND materia = S.materia and grupo = S.grupo and dia_semana = 7),'') sabado  ,
isnull((select apellido_paterno +' '+apellido_materno+' '+nombre_empleado 
from personal 
where rfc = (select rfc from grupos where periodo = S.periodo and materia = S.materia and grupo = S.grupo)),'Sin Asignar') docente , '' cadena_alumno, '' qr_alumno, '' cadena_dep, '' qr_dep , '' id_valida_documento, '' qr_valida_documento 
From [bdtec].[dbo].[alumnos] A inner join seleccion_materias S on 
(A.no_de_control = S.no_de_control  ) 
UNION 
select A.no_de_control ,'' nombre, A.semestre, reticula, especialidad, '' nombre_carrera, '' plan_estudios,'TUT-01' materia,  'TUTORÍA' nom_materia, grupo,'N' REPETICION , 1 cr, isnull((select hora_inicio+' - '+ hora_fin+' / '+aula horario from tutorias_horarios where periodo = S.periodo AND  grupo = S.grupo and dia = 'LUN'),'') lunes , isnull((select hora_inicio+' - '+ hora_fin+' / '+aula horario from tutorias_horarios where periodo = S.periodo AND  grupo = S.grupo and dia = 'MAR'),'') martes  , isnull((select hora_inicio+' - '+ hora_fin+' / '+aula horario from tutorias_horarios where periodo = S.periodo AND  grupo = S.grupo and dia = 'MIE'),'') miercoles   , isnull((select hora_inicio+' - '+ hora_fin+' / '+aula horario from tutorias_horarios where periodo = S.periodo AND  grupo = S.grupo and dia = 'JUE'),'') jueves  , isnull((select hora_inicio+' - '+ hora_fin+' / '+aula horario from tutorias_horarios where periodo = S.periodo AND  
grupo = S.grupo and dia = 'VIE'),'') viernes  , '' sabado, isnull((select apellido_paterno +' '+apellido_materno+' '+nombre_empleado from personal where rfc = (select rfc from tutorias_grupos where periodo = S.periodo and grupo = S.grupo)),'SIN ASIGNAR') docente , '' cadena_alumno, '' qr_alumno, '' cadena_dep, '' qr_dep , '' id_valida_documento, '' qr_valida_documento
From [bdtec].[dbo].[alumnos] A inner join tutorias_inscritos S on (A.no_de_control = S.no_de_control )
ORDER by nombre desc</CommandText>
