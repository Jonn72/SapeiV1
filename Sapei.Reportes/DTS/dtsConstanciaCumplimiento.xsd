                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  C.no_de_control, periodo,(select  apellido_paterno +' '+ apellido_materno + ' '+ nombre_alumno from alumnos where no_de_control = C.no_de_control) as nombre
,(select  nombre_carrera from estudiantes_datos_completos where no_de_control = C.no_de_control) as carrera 
,semestre
,  (select UPPER(descripcion) from extra_actividades where id = actividad)descripcion ,
(select  upper(nombre + ' '+ paterno +' '+ materno)  from extra_entrenador where id = (select id_entrenador from extra_actividades where id = C.actividad)) entrenador,promedio,
case when (4 &gt;= promedio and promedio &gt;= 3.5) then 'EXCELENTE' when (3.5 &gt; promedio and promedio &gt;= 2.5) then 'NOTABLE' 
when (2.5 &gt; promedio and promedio &gt;= 1.5) then 'BUENO' when (1.5 &gt; promedio and promedio &gt;= 1) then 'SUFICIENTE'  end desempeño, ' ' folio, ' ' qr
From [bdtec].[dbo].[extra_actividades_inscritos] C </CommandText>
