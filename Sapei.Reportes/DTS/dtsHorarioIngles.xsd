                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>	select (select identificacion_corta from periodos_escolares where periodo = C.periodo) periodo 
		,no_de_control, (select nombre_alumno + ' ' + apellido_paterno + ' '+ apellido_materno from alumnos where no_de_control = C.no_de_control) nombre
		,nivel,grupo
, isnull((select hora_inicial+' - '+ hora_final +' / '+aula from cle_horarios where periodo = C.periodo and nivel = C.nivel and grupo = C.grupo and dia = 2),'') lunes
, isnull((select hora_inicial+' - '+ hora_final +' / '+aula from cle_horarios where periodo = C.periodo and nivel = C.nivel and grupo = C.grupo and dia = 3),'') martes
, isnull((select hora_inicial+' - '+ hora_final +' / '+aula from cle_horarios where periodo = C.periodo and nivel = C.nivel and grupo = C.grupo and dia = 4),'') miercoles
, isnull((select hora_inicial+' - '+ hora_final +' / '+aula from cle_horarios where periodo = C.periodo and nivel = C.nivel and grupo = C.grupo and dia = 5),'') jueves
, isnull((select hora_inicial+' - '+ hora_final +' / '+aula from cle_horarios where periodo = C.periodo and nivel = C.nivel and grupo = C.grupo and dia = 6),'') viernes
, isnull((select hora_inicial+' - '+ hora_final +' / '+aula from cle_horarios where periodo = C.periodo and nivel = C.nivel and grupo = C.grupo and dia = 7),'') sabado

		from cle_seleccion C </CommandText>
